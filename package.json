    "@babel/preset-typescript": "^7.10.1",
    "@types/node": "^14.0.11",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "rollup": "^2.15.0",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
