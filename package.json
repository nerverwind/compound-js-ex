{
  "name": "@compound-finance/compound-js",
  "browser": "dist/browser/compound.min.js",
  "version": "0.0.12",
  "description": "A JavaScript SDK for Ethereum and the Compound Protocol.",
  "main": "dist/nodejs/index.js",
  "scripts": {
    "build": "npx tsc && npm run rollup",
